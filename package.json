{
  "name": "rx",
  "version": "0.0.0",
  "description": "A React/Flux app generate by RF, powered with Babel",
  "main": "build/bundle.js",
  "dependencies": {
    "axios": "^0.5.4",
    "babel-preset-react": "^6.3.13",
    "browser-request": "^0.3.3",
    "debounce": "^1.0.0",
    "flux": "^2.0.3",
    "immutable": "^3.7.5",
    "json-loader": "^0.5.2",
    "less": "^2.5.3",
    "moment": "^2.10.6",
    "node-uuid": "^1.4.3",
    "object-assign": "^2.0.0",
    "react": "^0.13.3",
    "react-day-picker": "^1.0.5",
    "react-markdown": "^1.0.3",
    "request-promise": "^0.4.3",
    "striptags": "^2.0.3"
  },
  "devDependencies": {
    "babel-jest": "*",
    "babel-loader": "*",
    "babel-preset-es2015": "^6.3.13",
    "bootstrap": "*",
    "bootstrap-webpack": "*",
    "css-loader": "*",
    "file-loader": "*",
    "gulp": "^3.9.0",
    "gulp-gh-pages": "^0.5.2",
    "imports-loader": "*",
    "jest-cli": "*",
    "jquery": "*",
    "node-sass": "*",
    "react-hot-loader": "*",
    "react-tools": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "url-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "scripts": {
    "build": "webpack -p --config webpack.prod.config.js",
    "dev": "webpack-dev-server --host 0.0.0.0 --config webpack.dev.config.js --colors --content-base ./build",
    "test": "jest",
    "start": "webpack-dev-server --host 0.0.0.0 --config webpack.dev.config.js --colors --content-base ./build | node mock-cds-backend/index.js"
  },
  "author": "",
  "license": "",
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "globals": {
      "SCRIPTS": "<rootDir>/src/scripts"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
